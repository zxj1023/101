<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false" android:state_enabled="true">
        <layer-list>
            <item android:drawable="@drawable/talpaossdk_switch_thumb_off"/>
        </layer-list>
    </item>
    <item android:state_checked="true" android:state_enabled="true">
        <layer-list>
            <item android:drawable="@drawable/talpaossdk_switch_thumb_on"/>
        </layer-list>
    </item>
    <item android:state_checked="false" android:state_enabled="false">
        <layer-list>
            <item android:drawable="@drawable/talpaossdk_switch_thumb_off_disable"/>
        </layer-list>
    </item>
    <item android:state_checked="true" android:state_enabled="false">
        <layer-list>
            <item android:drawable="@drawable/talpaossdk_switch_thumb_on"/>
        </layer-list>
    </item>
    <item android:drawable="@drawable/talpaossdk_switch_thumb_off"/>
</selector>

