<?xml version="1.0" encoding="utf-8"?>
<com.transsion.ossdk.dialog.view.TalpaOssdkDialogCustomeEditContainer xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/talpaossdk_dialog_edit_container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingLeft="@dimen/talpaossdk_dialog_et_container_paddingLR"
    android:paddingRight="@dimen/talpaossdk_dialog_et_container_paddingLR"
    android:layout_marginTop="@dimen/talpaossdk_dialog_et_content_marginTB">

    <TextView
        android:id="@+id/talpaossdk_dialog_secodetitle_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/talpaossdk_dialog_et_title_textsize"
        android:textColor="@color/talpaossdk_dialog_content_textcolor"
        android:fontFamily="roboto-regular"
        android:layout_marginBottom="@dimen/talpaossdk_dialog_et_secondtitle_marginbottom"
        android:visibility="gone"/>

    <EditText
        android:id="@+id/talpaossdk_dialog_et"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/talpaossdk_dialog_et_content_marginTB"
        android:background="@drawable/talpaossdk_dialog_et_bg"
        android:minHeight="@dimen/talpaossdk_dialog_et_edittext_minheight"
        android:paddingBottom="@dimen/talpaossdk_dialog_et_edittext_paddingTB"
        android:paddingLeft="@dimen/talpaossdk_dialog_et_edittext_paddingLeft"
        android:paddingTop="@dimen/talpaossdk_dialog_et_edittext_paddingTB"
        android:textColor="@color/talpaossdk_dialog_content_textcolor"
        android:textSize="@dimen/talpaossdk_dialog_content_textsize"
        android:textCursorDrawable="@drawable/talpaossdk_dialog_text_cursor_material"/>
</com.transsion.ossdk.dialog.view.TalpaOssdkDialogCustomeEditContainer>