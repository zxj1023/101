 <layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/aurora_background"
		>
		<inset android:drawable="@drawable/aurora_seekbar_bg"
		android:insetTop="3.5dp"
		android:insetBottom="3.5dp"/>
    </item>
    <item android:id="@+id/aurora_secondaryProgress"
		>
        <clip>
            <shape>
                <corners android:radius="4dip" />
                <padding   
		        android:left="3dp"  
		        android:top="3dp"   
		        android:right="3dp"  
		        android:bottom="3dp" />
                <gradient
                    android:startColor="#4ee2a6"
                    android:centerColor="#ff4ee2a6" 
                    android:centerY="0.2"
                    android:endColor="#88da3e"
                    android:angle="0"
                />
            </shape>
        </clip>
    </item>
    <item android:id="@+id/aurora_progress">
        <clip>
            <shape>
                <corners android:radius="4dip" />
                <stroke android:width="7dp" color="#e1e1e1" />
                <padding   
		        android:left="3dp"  
		        android:top="3dp"   
		        android:right="3dp"  
		        android:bottom="3dp" />
                <gradient
                    android:startColor="#4ee2a6"
                    android:centerColor="#ff4ee2a6"
                    android:centerY="0.2"
                    android:endColor="#88da3e"
                    android:angle="0"
                />
            </shape>
        </clip>
    </item>
</layer-list>

